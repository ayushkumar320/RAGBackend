{
  "name": "rag-backend",
  "version": "1.0.0",
  "description": "Backend architecture for RAG Based PPT Screener",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon --watch src --exec \"npm run build && npm start\" -e ts",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "RAG",
    "Node"
  ],
  "author": "ayushkumar320",
  "license": "ISC",
  "dependencies": {
    "@types/dotenv": "^6.1.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^9.1.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.3",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3"
  }
}